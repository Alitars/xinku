/**index.wxss**/

page{
  background-color: rgba(180, 216, 238, 1);
}
.container{
  height: 100vh;
}
.logo-wrap{
  position: absolute;
  height: 332rpx;
  width: 100vw;
  background-color: #D9D9D9;
}
.logo{
  padding: 40rpx 0;
  text-align: center;
}
.tabs-wrap{
  width: 600rpx;
  margin-left: 75rpx;
}
.form{
  background-color: white;
  padding:  20rpx 0 40rpx 0;
}
.form-item{
  padding-bottom: 20rpx;
}
.van-checkbox__label{
  font-size: 26rpx;
}
/* 样式覆盖 */
.van-tabs__line{
  width: 60px!important;
  left: -10px!important;
  bottom: 6rpx !important;
}
.nav{
  font-size: 40rpx;
  /* height: 80rpx; */
}
.default-tab{
  /* background-color: rgba(210, 180, 180, 1); */
  background-color: #A5CEEE;
}
.tab-active{
  background-color: white;
}

.a-text{
  color: lightskyblue;
  font-size: 24rpx;
}
.flex-container{
  display: flex;
  padding-bottom: 40rpx;
  font-size: 28rpx;
}
.accept-info{
  padding: 0 16px;
}
.regist-wrap{
  padding-left: 40rpx;
  line-height: 24rpx;
}
.login-btn{
  width: calc( 100% - 60px);
  margin-left: 30px;
  margin-top: 40rpx;
}

.popup{
  width: 600rpx;
  padding: 40rpx 40rpx 0;
  word-wrap:break-word;
}

.protocol-title{
  text-align: center;
  margin-bottom: 40rpx;
}

.comfirm-btn{
  margin: 60rpx 0;
}

.rigst-btn{
  width: calc(100% - 16px);
  margin-top:7px;
}
.topimg{
  width:100%;
  height: 400rpx;
  padding:0;
  display: block;
}
.van-checkbox__label--disabled {
  color: rgb(135, 206, 250)!important;
}
.van-button--large {
  color: #333!important;
}
