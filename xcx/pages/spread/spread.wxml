<view class="contain">
  <view class="clearfix tit">
    <view class="fl tip">推荐对象</view>
    <view class="fl tip1">状态</view>
    <view class="fl tip2">推荐/注册时间</view>
  </view>
  <block wx:for="{{list}}" wx:key="index">
    <view class="clearfix tit list">
      <view class="fl tip ellipsis">{{item.type==1?item.name:item.orgName}}</view>
      <view class="fl tip1">{{item.regStatus ==1?'待注册':'注册成功'}}</view>
      <view class="fl tip2">{{item.createDate}}</view>
    </view>
  </block>
</view>
